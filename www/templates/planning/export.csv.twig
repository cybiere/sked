{% spaceless %}"",{% endspaceless %}
{% for user in calendars|keys %}
{% spaceless %}"{{ user }}"{% if not loop.last %},{% endif %}{% endspaceless %}
{% endfor %}

{% for key, period in calendar %}
{% spaceless %}"{{ period|replace({'am2': "10h-12h", 'am': "08h-10h", 'pm2': "15h-17h", 'pm': "13h-15h"}) }}",{% endspaceless %}
{% for users in calendars %}
{% spaceless %}"{% if key in users|keys %}{{ users[key] }}{% endif %}"{% if not loop.last %},{% endif %}{% endspaceless %}
{% endfor %}

{% endfor %}
